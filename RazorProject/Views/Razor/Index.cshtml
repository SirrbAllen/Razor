<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <div>
        @if (Model != null)
        {
            <a href="Create" id="Button">Add Technology</a>
                foreach (var item in Model)
                {
                <div>
                    <h1>@Html.Encode(item.Title)</h1>
                    <h3>@Html.Encode(item.Description)</h3>
                    <h3>@Html.Encode(item.Priority)</h3>
                    <h3>@Html.Encode(item.Category)</h3>
                    <h3>@Html.Encode(item.Status)</h3>
                </div>
            }
        }

</div>
</body>
</html>

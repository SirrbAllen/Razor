<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <div> 
        @if (Model != null)
        {
            <a href="Create">Create</a>
            <table>
                <tr>
                    <th>Name</th>
                    <th>Description</th>
                    <th>Priority</th>
                </tr>
            </table>
                foreach (var item in Model)
                {
                <table>
                    <tr>
                        <td>@Html.Encode(item.Title)</td>
                        <td>@Html.Encode(item.Description)</td>
                        <td>@Html.Encode(item.Priority)</td>
                        <td>@Html.Encode(item.Status)</td>
                        <td>@Html.Encode(item.Category)</td>
                        <td>@Html.Encode(item.Url)</td>
                    </tr>
                </table>
                }
                }

</div>
</body>
</html>
